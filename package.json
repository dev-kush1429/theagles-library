{
  "name": "mc-components",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index",
  "peerDependencies": {
    "@storybook/react": "^5.3.17",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "react": "^16.13.1"
  },
  "dependencies": {
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-docs": "^5.3.17",
    "@storybook/addon-knobs": "^5.3.17",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/preset-create-react-app": "^2.1.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@tinymce/tinymce-react": "^3.6.0",
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "react-bootstrap": "^1.0.0",
    "react-color": "^2.18.1",
    "react-fontawesome": "^1.7.1",
    "react-loading": "^2.0.3",
    "react-router-dom": "^5.1.2",
    "react-uuid": "^1.0.2",
    "styled-components": "^5.1.0",
    "styled-icons": "^10.2.1",
    "yup": "^0.28.3"
  },
  "scripts": {
    "local": "STORYBOOK_STORY_MODE=true start-storybook -s ./public -p 9000",
    "start": "STORYBOOK_STORY_MODE=true start-storybook -s ./public",
    "build": "build-storybook -c .storybook -o .out",
    "test-components": "export PORT=5000 && REACT_APP_TEST_MODE=true SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "build-components": "babel --no-babelrc src --out-dir dist --ignore mock --copy-files --presets=env,stage-2,react",
    "build-react": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "jest",
    "test:c": "jest --coverage ",
    "test:u": "jest --coverage --updateSnapshot"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "react-dom": "^16.13.1",
    "react": "^16.13.1",
    "react-scripts": "3.4.1",
    "@babel/core": "^7.10.5",
    "@storybook/react": "^5.3.17",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "enzyme": "^3.11.0",
    "jest-enzyme": "^7.1.2",
    "jest-sonar-reporter": "^2.0.0",
    "jest": "^26.1.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss|sss|styl)$": "<rootDir>/node_modules/jest-css-modules"
    }
  }
}
